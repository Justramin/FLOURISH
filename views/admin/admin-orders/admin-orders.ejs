      <div class="page-content">
        <!-- Page Header-->
        <div class="page-header no-margin-bottom">
          <div class="container-fluid">
            <h2 class="h5 no-margin-bottom">Orders Manage</h2>
          </div>
        </div>
        <!-- Breadcrumb-->
        <div class="container-fluid">
          <ul class="breadcrumb">
            <li class="breadcrumb-item"><a href="/admin">Home</a></li>
            <li class="breadcrumb-item active">Orders</li>
          </ul>
        </div>
        <section class="no-padding-top">
          <div class="container-fluid">
            <div class="col-lg-12">
                <div class="block">
                  <div class="title"><strong>FLOURISH ORDERS MANAGEMENT</strong></div>
                  <div class="table-responsive"> 
                    <table class="table table-striped table-hover">
                      <thead>
                        <tr>
                          <th>SI No:</th>
                          <th>order ID</th>
                          <th>User Name</th>
                          <th>Payment Methode</th>
                          <th>Order Date</th>
                          <th>Status</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        
                          <% for( let i = 0; i < data.length; i++ ) { %>
                            <tr>
                            <th scope="row"><%= i+1  %></th>
                          <td><%= data[i].orderID  %></td>
                          <td><%= data[i].user  %></td>
                          <th>COD</th>
                          <th><%= data[i].date.toLocaleDateString()  %></th>
                          <td><%= data[i].status  %> </td>
                          <td><a class="btn btn-primary" style="color: white;" href="/admin/adminOrderDetail/<%=  data[i].orderID  %>">Details</a></td>
                        </tr>
                          <% } %>
                          
                        
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
          </div>
        </section>
        <footer class="footer">
          <div class="footer__block block no-margin-bottom">
